@model PartnerTotalsDTO

<div class="pb-2">
    <span class="main-font h4 font-weight-bold">Сводная информация:</span>
</div>
<div class="bg-main-light rounded-1">
    <div class="container-fluid px-3 py-4">
        <div class="row m-0 pb-4">
            <div class="col-12 px-2">
                <label class="main-font h4 font-weight-light">Итого единиц на складе:</label>
            </div>
            <div class="col-12 px-2">
                <label class="main-font h1 font-weight-bold">@Model.Amount единиц</label>
            </div>
        </div>
        <div class="row m-0 pb-4">
            <div class="col-12 px-2">
                <label class="main-font h4 font-weight-light">Итого моделей на складе:</label>
            </div>
            <div class="col-12 px-2">
                <label class="main-font h1 font-weight-bold">@Model.ModelsQuantity моделей</label>
            </div>
        </div>
        <div class="row m-0">
            <div class="col-12 px-2">
                <label class="main-font h4 font-weight-light">Итого к выплате по продажам:</label>
            </div>
            <div class="col-12 px-2">
                <label class="main-font h1 font-weight-bold">@Model.TotalToPay.ToTwoDecimalPlaces()$</label>
            </div>
        </div>
    </div>
</div>
<partial name="_ReportsNavigation"/>
