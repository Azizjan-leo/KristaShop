        </div>
    </div>
</div>
@* Marquee *@
<div class="row my-4 my-lg-5 mx-0">
    <div class="container-fluid p-0 bg-gradient-exclusive py-3">
        <div class="row m-0">
            <div class="col-auto text-center m-auto px-4">
                <img src="/common/img/svg/krista-pic-transparent-sm.gif" alt="Krista Diamond">
            </div>
            <div class="col overflow-hidden pl-2 pl-lg-3 pr-0">
                <div class="marquee my-2" data-duplicated="true" data-direction="left">
                    @foreach (var letter in citiesNames) {
                        <div class="letter-pane letter-@((int) letter)">@letter</div>
                    }
                </div>
            </div>
        </div>
    </div>
</div>
