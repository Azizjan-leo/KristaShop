@model (KristaShop.Common.Models.Filters.ReportsFilter filter, Module.Common.Business.Models.ReportTotalsDTO totals, List<Module.Order.Business.Models.CartItem1CDTO> items)
@{
    ViewData["Title"] = "Расшифровка по моделям (В корзинах)";
}
<div class="row m-0 mb-4">
    <div class="col-12 px-0 pb-4">
        <h4 class="h4 font-weight-bold">@ViewData["Title"]</h4>
    </div>
    <div class="col-12 px-0">
        <div class="dataTables_customTotalsWrapper p-4 mb-2">
            <partial name="Reports/_ReportTotals" model="Model.totals" />
        </div>
    </div>
</div>
<form>
    <div class="dataTables_customSearchWrapper p-0 border-0">
        <partial name="Reports/_ReportFilter" model="Model.filter" />
        <div class="form-row justify-content-end">
            <div class="form-group col-4 mb-2">
                <button type="submit" class="btn btn-main-primary btn-block">Применить фильтр</button>
            </div>
        </div>
    </div>
</form>
<div class="col-12 table-responsive">
    <table class="table table-striped table-bordered text-break" style="width: 100%">
        <thead>
            <tr>
                <th></th>
                <th>Информация о модели</th>
                <th>Артикул</th>
                <th>Клиенты Id</th>
                <th>Менеджеры Id</th>
                <th>Города Id</th>
                <th>Цвет Id</th>
                <th>Цвета</th>
                <th>Линейки</th>
                <th>Единицы</th>
                <th>На сумму</th>
                <th>Каталог ID</th>
                <th>Каталог</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    <script type="text/javascript">
        var dataModel = @Html.AsJsObject(Model.items);
    </script>
    <script src="/cart-admin/js/carts-report.js" asp-append-version="true" type="text/javascript"></script>
}