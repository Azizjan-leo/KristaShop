@{
    ViewData["Title"] = "Производство";
}
<partial name="_LinkPartial" />
<partial name="_SearchManufacturePartial" model="null"/>

<div class="row">
    <div class="col-12 pb-4 pt-5">
        <h4 class="h4 font-weight-bold" style="width: 100%;">@ViewData["Title"] <small style="float: right;">ИТОГО:  <strong class="jsTotalCount">0</strong> ед. <strong class="jsTotalSum">0.00</strong> $</small></h4>
    </div>
</div>

<div class="col-12 table-responsive">
    <table class="table table-striped table-bordered text-break" style="width:100%">
        <thead>
            <tr>
                <th>ФИО клиента</th>
                <th>Город</th>
                <th>Менеджер</th>
                <th>Единиц</th>
                <th>Сумма ($)</th>
                <th>Сумма (р)</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Styles {
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/tempusdominus-bootstrap-4/5.0.1/css/tempusdominus-bootstrap-4.min.css" />
}

@section Scripts {
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.28.0/moment.min.js" integrity="sha512-Q1f3TS3vSt1jQ8AwP2OuenztnLU6LwxgyyYOG1jgMW/cbEMHps/3wjvnl1P3WTrF3chJUWEoxDUEjMxDV8pujg==" crossorigin="anonymous"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/tempusdominus-bootstrap-4/5.0.1/js/tempusdominus-bootstrap-4.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.28.0/locale/ru.min.js"></script>
    <script src="/order-admin/js/Orders/Manufacture.js" asp-append-version="true" type="text/javascript"></script>
    <script src="/common-admin/js/datetime-picker.js" asp-append-version="true" type="text/javascript"></script>
}